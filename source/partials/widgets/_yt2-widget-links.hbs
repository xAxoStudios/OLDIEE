<!-- Widget: Unbox Links -->
<div>
  <h4
    class="mb-6 pt-[2px] font-heading text-lg font-bold leading-none text-primary dark:text-white md:mb-16"
  >Unbox Links</h4>
  <ul
    class="grid grid-cols-2 gap-x-7 gap-y-3 font-base text-sm font-bold leading-normal"
  >
    {{#each yt2.footer.links}}
      <li>
        <a
          class="transition-colors {{#ifpage url}}text-accent dark:text-accent {{/ifpage}} {{#unlesspage url}} text-primary hover:text-accent dark:text-white dark:hover:text-accent{{/unlesspage}}"
          href="{{url}}.html"
        >
          {{label}}
        </a>
      </li>
    {{/each}}
  </ul>
</div>
<!-- Widget: Unbox Links / End -->
