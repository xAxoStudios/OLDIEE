{{#if gmap}}
<script async src="https://maps.googleapis.com/maps/api/js?key={{global.gmapkey}}&callback=initMap"></script>
<script>
  let map = document.getElementById("vv-venue-map");
  let gmap;

  function initMap() {
    gmap = new google.maps.Map(map, {
      center: { lat: 40.767956464620504, lng: -73.98931002607559 },
      zoom: 15,
      disableDefaultUI: true,
    });
  }

  window.initMap = initMap;
</script>
